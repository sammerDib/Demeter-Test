<?xml version="1.0" encoding="utf-8"?>
<FlowsConfiguration xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <Flows>
    <FlowConfigurationBase xsi:type="AFCameraConfiguration">
      <WriteReportMode>NeverWrite</WriteReportMode>
      <Speed>Normal</Speed>
      <MeasureNbToQualityScore>50</MeasureNbToQualityScore>
      <SmallRangeCoeff>0.1</SmallRangeCoeff>
      <MediumRangeCoeff>0.5</MediumRangeCoeff>
      <LargeRangeCoeff>1</LargeRangeCoeff>
      <FactorBetweenDepthOfFieldAndStepSize>0.01</FactorBetweenDepthOfFieldAndStepSize>
      <CameraFramerateLimiter>1</CameraFramerateLimiter>
      <CameraNbLinesAOI>128</CameraNbLinesAOI>
      <PiezoStep Value="90" Unit="Nanometer" />
      <MinZStep Value="0.6" Unit="Micrometer" />
      <MaxZStep Value="25" Unit="Micrometer" />
      <PositionTrackingPeriod_ms>10</PositionTrackingPeriod_ms>
      <AutoFocusScanRange Value ="2" Unit="Millimeter" />
    </FlowConfigurationBase>
    <FlowConfigurationBase xsi:type="AFLiseConfiguration">
      <WriteReportMode>NeverWrite</WriteReportMode>
      <Speed>Normal</Speed>
      <ZPosToSignalIndexCoeff>2600</ZPosToSignalIndexCoeff>
      <MinDistanceToAvoidInterference>7</MinDistanceToAvoidInterference>
      <AutoFocusScanRange Value ="2" Unit="Millimeter" />
    </FlowConfigurationBase>
    <FlowConfigurationBase xsi:type="AirGapLiseConfiguration">
      <WriteReportMode>NeverWrite</WriteReportMode>
    </FlowConfigurationBase>
    <FlowConfigurationBase xsi:type="AlignmentMarksConfiguration">
      <WriteReportMode>NeverWrite</WriteReportMode>
    </FlowConfigurationBase>
    <FlowConfigurationBase xsi:type="AutolightConfiguration">
      <WriteReportMode>NeverWrite</WriteReportMode>
    </FlowConfigurationBase>
    <FlowConfigurationBase xsi:type="BareWaferAlignmentConfiguration">
      <DistanceToAvoidCameraInterference Value ="27" Unit="Millimeter" />
      <LightBoostPercentage>60</LightBoostPercentage>
      <CannyThreshold>200</CannyThreshold>
      <EdgeDetectionVersion>2</EdgeDetectionVersion>
      <NotchDetectionVersion>3</NotchDetectionVersion>
      <ReportOption>OverlayReport</ReportOption>
      <NotchWidth Value ="3000" Unit="Micrometer" />
      <WriteReportMode>NeverWrite</WriteReportMode>
    </FlowConfigurationBase>
    <FlowConfigurationBase xsi:type="BareWaferAlignmentImageConfiguration">
      <CannyThreshold>200</CannyThreshold>
      <EdgeDetectionVersion>2</EdgeDetectionVersion>
      <WriteReportMode>NeverWrite</WriteReportMode>
    </FlowConfigurationBase>
    <FlowConfigurationBase xsi:type="CheckPatternRecConfiguration">
      <WriteReportMode>NeverWrite</WriteReportMode>
    </FlowConfigurationBase>
    <FlowConfigurationBase xsi:type="DieAndStreetSizesConfiguration">
      <WriteReportMode>NeverWrite</WriteReportMode>
      <Speed>Normal</Speed>
      <OverlapForNextDieResearch>0.4</OverlapForNextDieResearch>
      <AdditionalEdgeExclusion Value ="7.5" Unit="Millimeter" />
    </FlowConfigurationBase>
    <FlowConfigurationBase xsi:type="EllipseCriticalDimensionConfiguration">
      <WriteReportMode>NeverWrite</WriteReportMode>
      <CannyThreshold>100</CannyThreshold>
    </FlowConfigurationBase>
    <FlowConfigurationBase xsi:type="CircleCriticalDimensionConfiguration">
      <WriteReportMode>NeverWrite</WriteReportMode>
      <CannyThreshold>300</CannyThreshold>
      <UseScharrAlgorithm>true</UseScharrAlgorithm>
      <UseMorphologicalOperations>true</UseMorphologicalOperations>
    </FlowConfigurationBase>
    <FlowConfigurationBase xsi:type="ImagePreprocessingConfiguration">
      <WriteReportMode>NeverWrite</WriteReportMode>
      <Speed>Normal</Speed>
    </FlowConfigurationBase>
    <FlowConfigurationBase xsi:type="MeasureDualLiseConfiguration">
      <WriteReportMode>NeverWrite</WriteReportMode>
    </FlowConfigurationBase>
    <FlowConfigurationBase xsi:type="MeasureLiseConfiguration">
      <WriteReportMode>NeverWrite</WriteReportMode>
    </FlowConfigurationBase>
    <FlowConfigurationBase xsi:type="MultipleMeasuresLiseConfiguration">
      <WriteReportMode>NeverWrite</WriteReportMode>
    </FlowConfigurationBase>
    <FlowConfigurationBase xsi:type="PatternRecConfiguration">
      <WriteReportMode>NeverWrite</WriteReportMode>
      <Speed>Normal</Speed>
      <AngleTolerance>30.0</AngleTolerance>
      <ScaleTolerance>0.02</ScaleTolerance>
      <DilationMaskSize>7</DilationMaskSize>
      <SimilarityThreshold>0.5</SimilarityThreshold>
    </FlowConfigurationBase>
    <FlowConfigurationBase xsi:type="PSIConfiguration">
      <PiezoMinStabilisationTime_ms>500</PiezoMinStabilisationTime_ms>
      <PiezoPositionStabilisationAccuracy Value="10" Unit="Nanometer" />
      <WriteReportMode>NeverWrite</WriteReportMode>
    </FlowConfigurationBase>
    <FlowConfigurationBase xsi:type="VSIConfiguration">
      <LambdaCenter Value="621.0" Unit="Nanometer" />
      <FwhmLambda Value="124.0" Unit="Nanometer" />
      <NoiseLevel>0.5</NoiseLevel>
      <MaskThreshold>0.0</MaskThreshold>
      <PiezoMinStabilisationTime_ms>30</PiezoMinStabilisationTime_ms>
      <PiezoPositionStabilisationAccuracy Value="10" Unit="Nanometer" />
      <WriteReportMode>NeverWrite</WriteReportMode>
    </FlowConfigurationBase>
    <FlowConfigurationBase xsi:type="TSVConfiguration">
      <WriteReportMode>NeverWrite</WriteReportMode>
      <Speed>Normal</Speed>
    </FlowConfigurationBase>
    <FlowConfigurationBase xsi:type="TSVDepthConfiguration">
      <WriteReportMode>NeverWrite</WriteReportMode>
      <Speed>Normal</Speed>
      <DetectionTolerance>10</DetectionTolerance>
    </FlowConfigurationBase>
    <FlowConfigurationBase xsi:type="WaferMapConfiguration">
      <WriteReportMode>NeverWrite</WriteReportMode>
      <Speed>Normal</Speed>
    </FlowConfigurationBase>
  </Flows>
</FlowsConfiguration>