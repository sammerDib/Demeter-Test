<?xml version="1.0" encoding="utf-8"?>
<PMConfiguration xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

  <Actor>DEMETER</Actor>
  
  <ToolKey>5</ToolKey>  
   
  <ChamberKey>1</ChamberKey>
  
  <PrealignementAngle>90</PrealignementAngle>

  <OutputAcqServer>\\4SEEPSD-ACQ\ACQ_Results\Manual</OutputAcqServer>

