<UserControl x:Class="UnitySC.PM.DMT.Modules.Settings.View.Designer.Designer"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Height="Auto" Width="Auto" SizeChanged="UserControl_SizeChanged" >
    <Border BorderBrush="#FF000000" BorderThickness="0" x:Name="OuterBorder">
		<Canvas ClipToBounds="True" PreviewMouseDown="DesignArea_PreviewMouseDown" x:Name="DesignArea" Background="#FFFFFFFF" ScrollViewer.HorizontalScrollBarVisibility="Auto" ScrollViewer.VerticalScrollBarVisibility="Auto"/>
	</Border>
</UserControl>
