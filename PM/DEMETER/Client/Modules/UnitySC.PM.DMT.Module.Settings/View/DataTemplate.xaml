    </DataTemplate>
    <DataTemplate DataType="{x:Type VmSettings:ExposureVM}">
        <ViewSettings:ExposureView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type VmSettings:CurvatureDynamicsVM}">
        <ViewSettings:CurvatureDynamicsView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type VmSettings:HighAngleDarkFieldMaskVM}">
        <ViewSettings:HighAngleDarkFieldMaskView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type VmSettings:GlobalTopoVM}">
        <ViewSettings:GlobalTopoView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type VmSettings:FlatFieldVM}">
        <ViewSettings:FlatFieldView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type VmSettings:ParametersVM}">
        <ViewSettings:ParametersView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type VmSettings:SystemUniformityVM}">
        <ViewSettings:SystemUniformityView />
    </DataTemplate>
    
</ResourceDictionary>