<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:VmSettings="clr-namespace:UnitySC.PM.DMT.Modules.Settings.ViewModel"
                    xmlns:ViewSettings="clr-namespace:UnitySC.PM.DMT.Modules.Settings.View">


    <DataTemplate DataType="{x:Type VmSettings:AlignmentVM}">
        <ViewSettings:AlignmentView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type VmSettings:CameraFocusVM}">
        <ViewSettings:CameraFocusView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type VmSettings:DeadPixelsVM}">
