<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:UnitySC.PM.EME.Client.Modules.TestAlgo.ViewModel"
                    xmlns:views ="clr-namespace:UnitySC.PM.EME.Client.Modules.TestAlgo.View"
                    xmlns:camera ="clr-namespace:UnitySC.PM.EME.Client.Controls.Camera;assembly=UnitySC.PM.EME.Client.Controls">

    <DataTemplate DataType="{x:Type camera:StandardCameraViewModel}">
        <camera:StandardCameraView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:AutoFocusVM}">
        <views:AutoFocusView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:PatternRecVM}">
        <views:PatternRecView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:VignettingVM}">
        <views:VignettingView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:PixelSizeComputationVM}">
        <views:PixelSizeComputationView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:DistortionVM}">
        <views:DistortionView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:AutoExposureVM}">
        <views:AutoExposureView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:DistanceSensorCalibrationVM}">
        <views:DistanceSensorCalibrationView />
    </DataTemplate>
</ResourceDictionary>