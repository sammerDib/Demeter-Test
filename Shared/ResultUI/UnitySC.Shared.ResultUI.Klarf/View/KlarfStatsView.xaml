<UserControl
	x:Class="UnitySC.Shared.ResultUI.Klarf.View.KlarfStatsView"
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:defect="clr-namespace:UnitySC.Shared.ResultUI.Common.View.Defect;assembly=UnitySC.Shared.ResultUI.Common"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	xmlns:viewModel="clr-namespace:UnitySC.Shared.ResultUI.Klarf.ViewModel"
	d:DataContext="{d:DesignInstance viewModel:KlarfStatsVM}"
	d:DesignHeight="450"
	d:DesignWidth="800"
	mc:Ignorable="d">
	<UserControl.Resources>
		<ResourceDictionary>
			<ResourceDictionary.MergedDictionaries>
				<ResourceDictionary Source="pack://application:,,,/UnitySC.Shared.ResultUI.Klarf;component/Resources.xaml" />
			</ResourceDictionary.MergedDictionaries>
		</ResourceDictionary>
	</UserControl.Resources>

	<!--  Defect view  -->
	<defect:DefectView />
</UserControl>