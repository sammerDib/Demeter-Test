<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:about="clr-namespace:UnitySC.GUI.Common.Vendor.Views.Panels.Help.About"
    xmlns:accessRights="clr-namespace:UnitySC.GUI.Common.Vendor.Views.Panels.Setup.AccessRights"
    xmlns:accessRightsUsers="clr-namespace:UnitySC.GUI.Common.Vendor.Views.Panels.Setup.AccessRights.Users"
    xmlns:alarmCenter="clr-namespace:UnitySC.GUI.Common.Vendor.Views.Panels.Setup.AlarmCenter"
    xmlns:analyzer="clr-namespace:UnitySC.GUI.Common.Vendor.Views.Panels.Maintenance.DataCollection.DataAnalysis.Analyzer"
    xmlns:appPathConfig="clr-namespace:UnitySC.GUI.Common.Vendor.Views.Panels.Setup.AppPathConfig"
    xmlns:appearance="clr-namespace:UnitySC.GUI.Common.Vendor.Views.Panels.Setup.Appearance"
    xmlns:businessPanelCommand="clr-namespace:UnitySC.GUI.Common.Vendor.Views.Panels.Help.Develop.BusinessPanelCommand"
    xmlns:controls="clr-namespace:UnitySC.GUI.Common.Vendor.Views.Panels.Help.Develop.Controls"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:dataAnalysis="clr-namespace:UnitySC.GUI.Common.Vendor.Views.Panels.Maintenance.DataCollection.DataAnalysis"
    xmlns:dataCollection="clr-namespace:UnitySC.GUI.Common.Vendor.Views.Panels.Setup.DataCollection"
    xmlns:dataSourceEditor="clr-namespace:UnitySC.GUI.Common.Vendor.Views.Panels.Maintenance.DataCollection.Editors.DataSourceEditor"
    xmlns:dataTable="clr-namespace:UnitySC.GUI.Common.Vendor.Views.Panels.Help.Develop.DataTable"
    xmlns:dataTree="clr-namespace:UnitySC.GUI.Common.Vendor.Views.Panels.Help.Develop.DataTree"
    xmlns:dataValidation="clr-namespace:UnitySC.GUI.Common.Vendor.Views.Panels.Help.Develop.DataValidation"
    xmlns:dataWriterEditor="clr-namespace:UnitySC.GUI.Common.Vendor.Views.Panels.Maintenance.DataCollection.Editors.DataWriterEditor"
    xmlns:dcpLibraryPopups="clr-namespace:UnitySC.GUI.Common.Vendor.Views.Panels.Maintenance.DataCollection.Library.Popups"
    xmlns:dcpPopups="clr-namespace:UnitySC.GUI.Common.Vendor.Views.Panels.Maintenance.DataCollection.ChartVisualization.Popups"
    xmlns:diagnostic="clr-namespace:UnitySC.GUI.Common.Vendor.Views.Panels.Setup.Diagnostic"
    xmlns:fileConfiguration="clr-namespace:UnitySC.GUI.Common.Vendor.Views.Panels.Setup.FileConfiguration"
    xmlns:fileViewer="clr-namespace:UnitySC.GUI.Common.Vendor.Views.Panels.Help.FileViewer"
    xmlns:globalSettings="clr-namespace:UnitySC.GUI.Common.Vendor.Views.Panels.Setup.GlobalSettings"
    xmlns:iconsViewer="clr-namespace:UnitySC.GUI.Common.Vendor.Views.Panels.Help.Develop.Resources.IconsViewer"
    xmlns:library="clr-namespace:UnitySC.GUI.Common.Vendor.Views.Panels.Maintenance.DataCollection.Library"
    xmlns:markDownViewer="clr-namespace:UnitySC.GUI.Common.Vendor.UIComponents.Components.MarkDownViewer"
    xmlns:markup="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:messageArea="clr-namespace:UnitySC.GUI.Common.Vendor.Views.Panels.Help.Develop.MessageArea"
    xmlns:navigateToPanelTester="clr-namespace:UnitySC.GUI.Common.Vendor.Views.Panels.Help.Develop.NavigateToPanelTester"
    xmlns:popup="clr-namespace:UnitySC.GUI.Common.Vendor.Views.Panels.Help.Develop.Popup"
    xmlns:popups="clr-namespace:UnitySC.GUI.Common.Views.Panels.Setup.DeviceSettings.Robot.RR75x.Popups"
    xmlns:randomAlarmGenerator="clr-namespace:UnitySC.GUI.Common.Vendor.Views.Tools.RandomAlarmGenerator"
    xmlns:realTimeAnalysis="clr-namespace:UnitySC.GUI.Common.Vendor.Views.Panels.Maintenance.DataCollection.RealTimeAnalysis"
    xmlns:recipeLibrary="clr-namespace:UnitySC.GUI.Common.Vendor.Views.Panels.Recipes.Library"
    xmlns:recipeRunner="clr-namespace:UnitySC.GUI.Common.Vendor.Views.Panels.Recipes.Runner"
    xmlns:rr75X="clr-namespace:UnitySC.GUI.Common.Views.Panels.Setup.DeviceSettings.Robot.RR75x"
    xmlns:salience="clr-namespace:UnitySC.GUI.Common.Vendor.Views.Panels.Help.Develop.Salience"
    xmlns:scenarioLibrary="clr-namespace:UnitySC.GUI.Common.Vendor.Views.Panels.Maintenance.Scenarios.Library"
    xmlns:scenarioRunner="clr-namespace:UnitySC.GUI.Common.Vendor.Views.Panels.Maintenance.Scenarios.Runner"
    xmlns:serviceMode="clr-namespace:UnitySC.GUI.Common.Vendor.Views.Panels.Maintenance.ServiceMode"
    xmlns:systemInformation="clr-namespace:UnitySC.GUI.Common.Vendor.Views.Panels.Help.SystemInformation"
    xmlns:themeBuilder="clr-namespace:UnitySC.GUI.Common.Vendor.Views.Tools.ThemeBuilder"
    xmlns:toolbox="clr-namespace:UnitySC.GUI.Common.Vendor.Views.Panels.Help.Develop.Toolbox"
    xmlns:traceViewer="clr-namespace:UnitySC.GUI.Common.Vendor.Views.Panels.Diagnostic.TraceViewer"
    xmlns:webVisu="clr-namespace:UnitySC.GUI.Common.Vendor.Views.Panels.Help.WebVisu"
    mc:Ignorable="d">

    <!--#region AgileoComponents-->

    <DataTemplate DataType="{markup:Type appPathConfig:AppPathConfigPanel}">
        <appPathConfig:AppPathConfigView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type alarmCenter:AlarmCenter}">
        <alarmCenter:AlarmCenterView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type globalSettings:GlobalSettingsPanel}">
        <globalSettings:GlobalSettingsView />
    </DataTemplate>

    <!--#region ServiceMode-->

    <DataTemplate DataType="{x:Type serviceMode:ServiceModePanel}">
        <serviceMode:ServiceModePanelView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type markDownViewer:MarkDownViewerViewModel}">
        <markDownViewer:MarkDownViewerView />
    </DataTemplate>

    <!--#endregion ServiceMode-->

    <DataTemplate DataType="{x:Type accessRights:AccessRightsPanel}">
        <accessRights:AccessRightsPanelView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type accessRightsUsers:UserEditorViewModel}">
        <accessRightsUsers:UserEditorView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type systemInformation:SystemInformationViewModel}">
        <systemInformation:SystemInformationView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type about:About}">
        <about:AboutView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type diagnostic:DiagnosticPanel}">
        <diagnostic:DiagnosticPanelView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type appearance:AppearancePanelViewModel}">
        <appearance:AppearancePanelView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type fileViewer:FileViewer}">
        <fileViewer:FileViewerView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type recipeLibrary:RecipeLibraryPanel}">
        <recipeLibrary:RecipeLibraryPanelView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type recipeRunner:RecipeRunnerPanel}">
        <recipeRunner:RecipeRunnerPanelView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type scenarioLibrary:ScenarioLibraryPanel}">
        <scenarioLibrary:ScenarioLibraryPanelView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type scenarioRunner:ScenarioRunnerPanel}">
        <scenarioRunner:ScenarioRunnerPanelView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type fileConfiguration:FileConfigurationPanel}">
        <fileConfiguration:FileConfigurationPanelView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type fileConfiguration:GroupsDefinitionEditorPopupContent}">
        <fileConfiguration:GroupsDefinitionEditorPopup />
    </DataTemplate>

    <DataTemplate DataType="{x:Type webVisu:WebVisuPanel}">
        <webVisu:WebVisuPanelView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type dcpPopups:SetupGraphPopup}">
        <dcpPopups:SetupGraphPopupView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type dataSourceEditor:EquipmentDataSourceEditor}">
        <dataSourceEditor:EquipmentDataSourceEditorView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type dataWriterEditor:DataWriterTypeSelectionPopup}">
        <dataWriterEditor:DataWriterTypeSelectionPopupView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type library:DataCollectionLibraryPanel}">
        <library:DataCollectionLibraryPanelView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type dcpLibraryPopups:FolderPathDetailsPopup}">
        <dcpLibraryPopups:FolderPathDetailsPopupView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type realTimeAnalysis:RealTimeAnalysisPanel}">
        <realTimeAnalysis:RealTimeAnalysisPanelView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type dataAnalysis:DataAnalysisPanel}">
        <ContentPresenter x:Name="presenter">
            <ContentPresenter.Resources>
                <DataTemplate DataType="{x:Type analyzer:ChartAnalyzerPanel}">
                    <analyzer:ChartAnalyzerPanelView />
                </DataTemplate>
                <DataTemplate DataType="{x:Type dataAnalysis:DataAnalysisPanel}">
                    <dataAnalysis:DataAnalysisPanelView />
                </DataTemplate>
            </ContentPresenter.Resources>
        </ContentPresenter>
        <DataTemplate.Triggers>
            <DataTrigger Binding="{Binding Path=ChartAnalyzer.IsInAnalyze}" Value="True">
                <Setter TargetName="presenter" Property="Content" Value="{Binding ChartAnalyzer}" />
            </DataTrigger>
        </DataTemplate.Triggers>
    </DataTemplate>

    <DataTemplate DataType="{x:Type dataCollection:DataCollectionSetupPanel}">
        <dataCollection:DataCollectionSetupPanelView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type traceViewer:TraceViewerPanel}">
        <traceViewer:TraceViewerPanelView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type dataTable:DataTablePanel}">
        <dataTable:DataTablePanelView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type dataTree:DataTreePanel}">
        <dataTree:DataTreePanelView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type popup:PopupPanel}">
        <popup:PopupPanelView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type controls:WpfControlsPanel}">
        <controls:WpfControlsPanelView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type controls:CustomControlsPanel}">
        <controls:CustomControlsPanelView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type businessPanelCommand:BusinessPanelCommandPanel}">
        <businessPanelCommand:BusinessPanelCommandPanelView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type messageArea:MessageAreaPanel}">
        <messageArea:MessageAreaPanelView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type navigateToPanelTester:NavigateToPanel}">
        <navigateToPanelTester:NavigateToPanelView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type salience:SaliencePanel}">
        <salience:SaliencePanelView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type toolbox:ToolboxPanel}">
        <toolbox:ToolboxPanelView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type iconsViewer:IconsViewerPanel}">
        <iconsViewer:IconsViewerPanelView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type dataValidation:DataValidationPanel}">
        <dataValidation:DataValidationPanelView />
    </DataTemplate>
    <!--#endregion AgileoComponents-->

    <!--#region Theme Editor Tools-->

    <DataTemplate DataType="{x:Type themeBuilder:ThemeBuilderTool}">
        <themeBuilder:ThemeBuilderToolView />
    </DataTemplate>

    <!--#endregion Theme Editor Tools-->

    <!--#region Random Alarm Generator Tools-->

    <DataTemplate DataType="{x:Type randomAlarmGenerator:RandomAlarmGeneratorTool}">
        <randomAlarmGenerator:RandomAlarmGeneratorToolView />
    </DataTemplate>

    <!--#endregion Random Alarm Generator Tools-->

</ResourceDictionary>
