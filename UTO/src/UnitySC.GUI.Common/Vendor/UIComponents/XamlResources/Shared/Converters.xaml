<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:converters="clr-namespace:UnitySC.GUI.Common.Vendor.UIComponents.Converters"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d">

    <!--#region .Net Converters-->
    <BooleanToVisibilityConverter x:Key="BooleanToVisibilityConverter" />
    <!--#endregion .Net Converters-->

    <!--#region Visibility converters-->

    <converters:NullToVisibilityConverter
        x:Key="NullToHiddenConverter"
        NotNullVisibility="Visible"
        NullVisibility="Hidden" />
    <converters:NullToVisibilityConverter
        x:Key="NotNullToHiddenConverter"
        NotNullVisibility="Hidden"
        NullVisibility="Visible" />
    <converters:NullToVisibilityConverter
        x:Key="NullToCollapsedConverter"
        NotNullVisibility="Visible"
        NullVisibility="Collapsed" />
    <converters:NullToVisibilityConverter
        x:Key="NotNullToCollapsedConverter"
        NotNullVisibility="Collapsed"
        NullVisibility="Visible" />

    <converters:BooleanToVisibilityConverter
        x:Key="FalseToHiddenConverter"
        FalseVisibility="Hidden"
        TrueVisibility="Visible" />
    <converters:BooleanToVisibilityConverter
        x:Key="FalseToCollapsedConverter"
        FalseVisibility="Collapsed"
        TrueVisibility="Visible" />
    <converters:BooleanToVisibilityConverter
        x:Key="TrueToHiddenConverter"
        FalseVisibility="Visible"
        TrueVisibility="Hidden" />
    <converters:BooleanToVisibilityConverter
        x:Key="TrueToCollapsedConverter"
        FalseVisibility="Visible"
        TrueVisibility="Collapsed" />

    <!--#endregion-->

    <!--#region Boolean converters-->
    <converters:NullToTrueConverter x:Key="NullToTrueConverter" />
    <converters:NullToFalseConverter x:Key="NullToFalseConverter" />
    <!--#endregion-->

    <converters:CollectionHasItemsToVisibilityConverter x:Key="CollectionHasItemsToVisibilityConverter" />
    <converters:FirstLetterUppercaseConverter x:Key="FirstLetterUppercaseConverter" />
    <converters:IsInstanceOfTypeConverter x:Key="IsInstanceOfTypeConverter" />
    <converters:LevelToLeftMarginConverter x:Key="LevelToLeftMarginConverter" LeftMarginByLevel="25" />
    <converters:MathMultipleConverter x:Key="MathMultiplierConverter" Operation="Multiply" />
    <converters:StringToBrushConverter x:Key="StringToBrushConverter" />
    <converters:TextToVisibilityConverter x:Key="TextToVisibilityConverter" />
    <converters:TreeViewItemToLeftMarginConverter x:Key="TreeViewItemToLeftMarginConverter" Length="20" />
    <converters:NotZeroConverter x:Key="NotZeroConverter" />
    <converters:AndOperatorMultiValueConverter x:Key="AndOperatorMultiValueConverter" />
    <converters:BooleansToVisibilityMultiValueConverter x:Key="BooleansToVisibilityMultiValueConverter" />
    <converters:BuzzerStateToIconConverter x:Key="BuzzerStateToIconConverter" />
    <converters:ColorToColorBrushConverter x:Key="ColorToColorBrushConverter" />
    <converters:DataItemToStringValueConverter x:Key="DataItemToStringValueConverter" />
    <converters:DateDisplayerFormatConverter x:Key="DateDisplayerFormatConverter" />
    <converters:DateTimeFormatConvert x:Key="DateTimeFormatConvert" />
    <converters:DoubleToHalfCornerRadiusConverter x:Key="DoubleToHalfCornerRadiusConverter" />
    <converters:DoubleToColumnWidthWithSeparationConverter x:Key="ColumnWidthWithSeparationConverter" />
    <converters:DurationToFormattedStringConverter x:Key="DurationToFormattedStringConverter" />
    <converters:EmptyCollectionToCollapsedConverter x:Key="EmptyCollectionToCollapsedConverter" />
    <converters:GreaterThanDoubleToVisibilityConverter x:Key="GreaterThanDoubleToVisibilityConverter" />
    <converters:IntModuloToVisibilityConverter x:Key="Int2ModuloToVisibilityConverter" Modulo="2" />
    <converters:IntModuloToVisibilityConverter
        x:Key="Int5ModuloToVisibilityConverter"
        FalseVisibility="Hidden"
        Modulo="5" />
    <converters:InvertedBoolConverter x:Key="InvertedBoolConverter" />
    <converters:InvertedEmptyCollectionToCollapsedConverter x:Key="InvertedEmptyCollectionToCollapsedConverter" />
    <converters:ItemIsInTheCollectionToBooleanConverter x:Key="ItemIsInTheCollectionToBooleanConverter" />
    <converters:ItemToCollectionIndexMultiConverter x:Key="ItemToCollectionIndexMultiConverter" />
    <converters:NaNDoubleToAutoStringConverter x:Key="NaNDoubleToAutoStringConverter" />
    <converters:ObjectToStringConverter x:Key="ObjectToStringConverter" />
    <converters:ObjectToTypeStringConverter x:Key="ObjectToTypeStringConverter" />
    <converters:OrOperatorMultiValueConverter x:Key="OrOperatorMultiValueConverter" />
    <converters:ProcessStartToBooleanConverter x:Key="ProcessStartToBooleanConverter" />
    <converters:QuantityToStringConverter x:Key="QuantityToStringConverter" />
    <converters:QuantityUnitToAbbreviationConverter x:Key="QuantityUnitToAbbreviationConverter" />
    <converters:RecipeComponentTypeToHumanizedTextConverter x:Key="RecipeComponentTypeToHumanizedTextConverter" />
    <converters:SampleDimensionToIntConverter x:Key="SampleDimensionToIntConverter" />
    <converters:TextToHumanizedAndUpperCaseTextConverter x:Key="ToHumanizedAndUpperCaseTextConverter" />
    <converters:TextToHumanizedTextConverter x:Key="TextToHumanizedTextConverter" />
    <converters:UnitToAbbreviationConverter x:Key="UnitToAbbreviationConverter" />
    <converters:WaitingThresholdOperatorToSignStringConverter x:Key="WaitingThresholdOperatorToSignStringConverter" />
    <converters:ListCountToVisibilityConverter x:Key="ListCountToVisibilityConverter" />
    <converters:ListToValueConverter x:Key="ListToValueConverter" />
    <converters:StartMethodToBooleanConverter x:Key="StartMethodToBooleanConverter" />
    <converters:ListViewItemToIndexConverter x:Key="ListViewItemToIndexConverter" />
    <converters:IsLastItemInContainerConverter x:Key="IsLastItemInContainerConverter" />
    <converters:EqualityMultiConverter x:Key="EqualityMultiConverter" />
    <converters:DeviceStateToBooleanConverter x:Key="DeviceStateToBooleanConverter" />
    <converters:TimeSpanToStringConverter x:Key="TimeSpanToStringConverter" />
    <converters:MinMultiConverter x:Key="MinMultiConverter" />
    <converters:ProportionalMultiConverter x:Key="ProportionalMultiConverter" />

</ResourceDictionary>
