<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:system="clr-namespace:System;assembly=mscorlib"
    mc:Ignorable="d">

    <system:Double x:Key="DisabledOpacity">0.3</system:Double>
    <system:Double x:Key="MouseOverOpacity">0.5</system:Double>
    <system:Double x:Key="ClickedOpacity">0.75</system:Double>

    <system:Double x:Key="DesignHeight">1018</system:Double>
    <system:Double x:Key="DesignWidth">1663</system:Double>

    <system:Double x:Key="HeaderElementHeight">40</system:Double>
    <system:Double x:Key="PanelMargin">7</system:Double>
    <system:Double x:Key="SubNavigationElementLeftMargin">20</system:Double>
    <GridLength x:Key="PanelMarginGridLength">7</GridLength>

    <Duration x:Key="RotationAnimationDuration">0:0:0.15</Duration>
    <KeyTime x:Key="RotationAnimationKeyTime">0:0:0.15</KeyTime>
    <Duration x:Key="ExpandAnimationDuration">0:0:0.15</Duration>
    <KeyTime x:Key="ExpandAnimationKeyTime">0:0:0.15</KeyTime>

    <Duration x:Key="ShowInteractionDuration">0:0:0.10</Duration>

    <system:Double x:Key="MaxCommandIconSize">32</system:Double>
    <system:Double x:Key="MaxSubCommandIconSize">20</system:Double>

    <system:Double x:Key="UnderlineThicknessSize">2</system:Double>
    <system:Double x:Key="UnactiveSortOpacity">0.38</system:Double>

    <system:Double x:Key="CardCornerRadiusDouble">5</system:Double>
    <system:Double x:Key="IconInsideBoxSizeDouble">17</system:Double>

</ResourceDictionary>
