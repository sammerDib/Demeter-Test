<UserControl
    x:Class="UnitySC.GUI.Common.Vendor.UIComponents.Components.MarkDownViewer.MarkDownViewerView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:markDownViewer="clr-namespace:UnitySC.GUI.Common.Vendor.UIComponents.Components.MarkDownViewer"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:wpf="clr-namespace:TheArtOfDev.HtmlRenderer.WPF;assembly=HtmlRenderer.WPF"
    d:DataContext="{d:DesignInstance markDownViewer:MarkDownViewerViewModel}"
    d:DesignHeight="450"
    d:DesignWidth="800"
    mc:Ignorable="d">
    <wpf:HtmlPanel
        Background="Transparent"
        Text="{Binding Content}" />
</UserControl>
