<UserControl
	x:Class="UnitySC.GUI.Common.Vendor.Views.Panels.Help.FileViewer.FileViewerView"
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:fileViewer="clr-namespace:UnitySC.GUI.Common.Vendor.Views.Panels.Help.FileViewer"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	d:DataContext="{d:DesignInstance fileViewer:FileViewer, IsDesignTimeCreatable=True}"
	d:DesignHeight="{StaticResource DesignHeight}"
	d:DesignWidth="{StaticResource DesignWidth}"
	Style="{StaticResource BusinessPanelViewStyle}"
	mc:Ignorable="d">
	<DocumentViewer Document="{Binding Path=Document, Mode=OneWay, UpdateSourceTrigger=PropertyChanged}" />
</UserControl>
