<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--#region Layout-->
    <Color x:Key="WindowBackground">#FF1D2124</Color>
    <Color x:Key="HorizontalCanvasBackground">#FF2E3136</Color>
    <Color x:Key="HorizontalCanvasForeground">#FFEEEEEE</Color>
    <Color x:Key="VerticalCanvasBackground">#FF2E3136</Color>
    <Color x:Key="VerticalCanvasForeground">#FFEEEEEE</Color>
    <Color x:Key="PanelBackground">#FF2E3136</Color>
    <Color x:Key="PanelForeground">#FFEEEEEE</Color>
    <Color x:Key="SurfaceBackground">#FFEEEEEE</Color>
    <Color x:Key="BackgroundMask">#4D000000</Color>
    <!--#endregion Layout-->

    <!--#region Control-->
    <Color x:Key="ControlActionBackground">#FF42454A</Color>
    <Color x:Key="ControlActionForeground">#FFEEEEEE</Color>
    <Color x:Key="ControlInputBackground">#FF42454A</Color>
    <Color x:Key="HeaderBackground">#FF42454A</Color>
    <Color x:Key="HeaderForeground">#FFEEEEEE</Color>
    <!--#endregion Control-->

    <!--#region Selection-->
    <Color x:Key="SelectionBackground">#322186AA</Color>
    <Color x:Key="SelectionForeground">#FF2186AA</Color>
    <Color x:Key="SelectionSourceForeground">#FF1f6484</Color>
    <Color x:Key="SelectionDestinationForeground">#FF6323B8</Color>
    <!--#endregion Selection-->

    <Color x:Key="RedColor">#ef534e</Color>
    <Color x:Key="PinkColor">#ec407a</Color>
    <Color x:Key="PurpleColor">#ab47bc</Color>
    <Color x:Key="DeepPurpleColor">#7e57c2</Color>
    <Color x:Key="IndigoColor">#5c6bc0</Color>
    <Color x:Key="BlueColor">#42a5f5</Color>
    <Color x:Key="DarkBlueColor">#000038</Color>
    <Color x:Key="LightBlueColor">#29b6f6</Color>
    <Color x:Key="CyanColor">#26c6da</Color>
    <Color x:Key="TealColor">#26a69a</Color>
    <Color x:Key="GreenColor">#66bb6a</Color>
    <Color x:Key="LightGreenColor">#9ccc65</Color>
    <Color x:Key="LimeColor">#d4e157</Color>
    <Color x:Key="YellowColor">#ffee58</Color>
    <Color x:Key="AmberColor">#ffca28</Color>
    <Color x:Key="OrangeColor">#ffa726</Color>
    <Color x:Key="DeepOrangeColor">#ff7043</Color>
    <Color x:Key="BrownColor">#8d6e63</Color>
</ResourceDictionary>
