<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--  The default lightweight theme is used at startup and then replaced by the user's theme  -->

    <!--#region Layout-->
    <Color x:Key="WindowBackground">#FFF7F7F7</Color>
    <Color x:Key="HorizontalCanvasBackground">#FFFFFFFF</Color>
    <Color x:Key="HorizontalCanvasForeground">#FF616161</Color>
    <Color x:Key="VerticalCanvasBackground">#FFFFFFFF</Color>
    <Color x:Key="VerticalCanvasForeground">#FF616161</Color>
    <Color x:Key="PanelBackground">#FFFFFFFF</Color>
    <Color x:Key="PanelForeground">#FF616161</Color>
    <Color x:Key="SurfaceBackground">#FFFFFFFF</Color>
    <Color x:Key="BackgroundMask">#4D000000</Color>
    <!--#endregion Layout-->

    <!--#region Control-->
    <Color x:Key="ControlActionBackground">#FFFFFFFF</Color>
    <Color x:Key="ControlActionForeground">#FF616161</Color>
    <Color x:Key="ControlInputBackground">#99D6D6D6</Color>
    <Color x:Key="HeaderBackground">#FFCDCDCD</Color>
    <Color x:Key="HeaderForeground">#FF5A5A5A</Color>
    <!--#endregion Control-->

    <!--#region Selection-->
    <Color x:Key="SelectionBackground">#FFE5E5E5</Color>
    <Color x:Key="SelectionForeground">#FFC20448</Color>
    <Color x:Key="SelectionSourceForeground">#FF1f6484</Color>
    <Color x:Key="SelectionDestinationForeground">#FF6323B8</Color>
    <!--#endregion Selection-->

    <Color x:Key="RedColor">#F44336</Color>
    <Color x:Key="PinkColor">#E91E63</Color>
    <Color x:Key="PurpleColor">#9C27B0</Color>
    <Color x:Key="DeepPurpleColor">#673AB7</Color>
    <Color x:Key="IndigoColor">#3F51B5</Color>
    <Color x:Key="BlueColor">#2196F3</Color>
    <Color x:Key="DarkBlueColor">#00008B</Color>
    <Color x:Key="LightBlueColor">#03A9F4</Color>
    <Color x:Key="CyanColor">#00BCD4</Color>
    <Color x:Key="TealColor">#009688</Color>
    <Color x:Key="GreenColor">#4CAF50</Color>
    <Color x:Key="LightGreenColor">#8BC34A</Color>
    <Color x:Key="LimeColor">#CDDC39</Color>
    <Color x:Key="YellowColor">#FFEB3B</Color>
    <Color x:Key="AmberColor">#FFC107</Color>
    <Color x:Key="OrangeColor">#FF9800</Color>
    <Color x:Key="DeepOrangeColor">#FF5722</Color>
    <Color x:Key="BrownColor">#795548</Color>
</ResourceDictionary>
