<UserControl
    x:Class="UnitySC.UTO.Controller.Views.Panels.Maintenance.ServiceMode.UnityServiceModePanelView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:serviceMode="clr-namespace:UnitySC.UTO.Controller.Views.Panels.Maintenance.ServiceMode"
    xmlns:serviceMode1="clr-namespace:UnitySC.GUI.Common.Vendor.Views.Panels.Maintenance.ServiceMode;assembly=UnitySC.GUI.Common"
    d:DataContext="{d:DesignInstance serviceMode:UnityServiceModePanel}"
    d:DesignHeight="450"
    d:DesignWidth="800"
    mc:Ignorable="d">
    <Grid IsEnabled="{Binding IsMaintenanceMode}">
        <serviceMode1:ServiceModePanelView DataContext="{Binding}" />
    </Grid>
</UserControl>
