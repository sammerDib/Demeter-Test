<UserControl
    x:Class="UnitySC.UTO.Controller.Views.Panels.Maintenance.Scenario.Runner.UnityScenarioRunnerPanelView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:runner="clr-namespace:UnitySC.UTO.Controller.Views.Panels.Maintenance.Scenario.Runner"
    xmlns:runner1="clr-namespace:UnitySC.GUI.Common.Vendor.Views.Panels.Maintenance.Scenarios.Runner;assembly=UnitySC.GUI.Common"
    d:DataContext="{d:DesignInstance runner:UnityScenarioRunnerPanel}"
    d:DesignHeight="{StaticResource DesignHeight}"
    d:DesignWidth="{StaticResource DesignWidth}"
    Style="{StaticResource BusinessPanelViewStyle}"
    mc:Ignorable="d">
    <runner1:ScenarioRunnerPanelView
        IsEnabled="{Binding IsMaintenanceMode}"
        Style="{x:Null}" />
</UserControl>
