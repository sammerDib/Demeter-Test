<UserControl
    x:Class="UnitySC.UTO.Controller.Views.Panels.Results.ResultsPanelView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:UnitySC.UTO.Controller.Views.Panels.Results"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:view="clr-namespace:UnitySC.Result.CommonUI.View;assembly=UnitySC.Result.CommonUI"
    d:DataContext="{d:DesignInstance local:ResultsPanel,
                                     IsDesignTimeCreatable=False}"
    d:DesignHeight="{StaticResource DesignHeight}"
    d:DesignWidth="{StaticResource DesignWidth}"
    Style="{StaticResource BaseBusinessPanelViewStyle}"
    mc:Ignorable="d">
    <view:MainResultView
        Margin="0,4,0,0"
        HorizontalAlignment="Stretch"
        VerticalAlignment="Stretch"
        DataContext="{Binding MainResult}" />
</UserControl>
