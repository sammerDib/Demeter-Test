<?xml version="1.0" encoding="utf-8"?>
<BrooksRobotConfiguration>
  <InitializationTimeout>300</InitializationTimeout>
  <UpperArm>
    <IsEnabled>true</IsEnabled>
    <EffectorType>VacuumU</EffectorType>
    <SupportedSubstrateSizes>
      <SampleDimension>S200mm</SampleDimension>
      <SampleDimension>S300mm</SampleDimension>
    </SupportedSubstrateSizes>
    <SupportedSubstrateTypes>
      <SubstrateType>Standard</SubstrateType>
    </SupportedSubstrateTypes>
  </UpperArm>
  <LowerArm>
    <IsEnabled>true</IsEnabled>
    <EffectorType>VacuumU</EffectorType>
    <SupportedSubstrateSizes>
      <SampleDimension>S200mm</SampleDimension>
      <SampleDimension>S300mm</SampleDimension>
    </SupportedSubstrateSizes>
    <SupportedSubstrateTypes>
      <SubstrateType>Standard</SubstrateType>
    </SupportedSubstrateTypes>
  </LowerArm>
  <RobotArmStoppingPositions>
    <SampleSizeStoppingPositions>
      <WaferSize>S300mm</WaferSize>
      <SampleSizeStoppingPositions>
        <StoppingPositionsPerWaferSize>
          <ModuleStoppingPositions>
            <Module>LoadPort1</Module>
            <ModuleStoppingPosition>
              <XPosition>125</XPosition>
              <ThetaPosition>90</ThetaPosition>
            </ModuleStoppingPosition>
          </ModuleStoppingPositions>
          <ModuleStoppingPositions>
            <Module>LoadPort2</Module>
            <ModuleStoppingPosition>
              <XPosition>420</XPosition>
              <ThetaPosition>90</ThetaPosition>
            </ModuleStoppingPosition>
          </ModuleStoppingPositions>
          <ModuleStoppingPositions>
            <Module>PreAlignerA</Module>
            <ModuleStoppingPosition>
              <XPosition>-100</XPosition>
              <ThetaPosition>180</ThetaPosition>
            </ModuleStoppingPosition>
          </ModuleStoppingPositions>
          <ModuleStoppingPositions>
            <Module>ProcessModuleA</Module>
            <ModuleStoppingPosition>
              <XPosition>100</XPosition>
              <ThetaPosition>0</ThetaPosition>
            </ModuleStoppingPosition>
          </ModuleStoppingPositions>
          <ModuleStoppingPositions>
            <Module>ProcessModuleB</Module>
            <ThetaPosition>270</ThetaPosition>
            <ModuleStoppingPosition>
              <XPosition>200</XPosition>
            </ModuleStoppingPosition>
          </ModuleStoppingPositions>
        </StoppingPositionsPerWaferSize>
      </SampleSizeStoppingPositions>
    </SampleSizeStoppingPositions>
  </RobotArmStoppingPositions>
  <BrooksRobotName>WaferEngine</BrooksRobotName>
  <UpperArmName>EFEM.WaferEngine.EE1</UpperArmName>
  <LowerArmName>EFEM.WaferEngine.EE2</LowerArmName>
  <UpperEndEffectorName>EE1</UpperEndEffectorName>
  <LowerEndEffectorName>EE2</LowerEndEffectorName>
  <RobotHomeMotionProfile>Home</RobotHomeMotionProfile>
</BrooksRobotConfiguration>
