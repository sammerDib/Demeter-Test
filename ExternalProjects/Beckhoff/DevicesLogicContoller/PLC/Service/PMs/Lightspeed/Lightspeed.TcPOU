  <POU Name="Lightspeed" Id="{b1820b5e-8283-43dc-b399-2d75c16eb6f0}" SpecialFunc="None">
    <Declaration><![CDATA[FUNCTION_BLOCK Lightspeed EXTENDS PlcModule
VAR_INPUT
	
	{attribute 'OPC.UA.DA' := '0'}
	LightspeedConfig : LightspeedConfig;
	
	{attribute 'OPC.UA.DA' := '0'}
	LsCalibrateConfig : LsCalibrateConfig;
	
	(* Chamber *)
	Chamber : LightspeedChamber(Config := LightspeedConfig.ChamberConfigs.LightspeedChamberConfig);
		
	(* AttenuationFilterConfigs *) 
	FW212CAttenuationFilter : FW212CAttenuationFilter(Config := LightspeedConfig.AttenuationFilterConfigs.FW212CAttenuationFilterConfig);
	ELL14KAttenuationFilter : ELL14KAttenuationFilter(Config := LightspeedConfig.AttenuationFilterConfigs.ELL14KAttenuationFilterConfig);
	ELL9AttenuationFilter : ELL9AttenuationFilter(Config := LightspeedConfig.AttenuationFilterConfigs.ELL9AttenuationFilterConfig);
	
	(* Slider Attenuation *)
	ELL6SliderAttenuationFilter : ELL6AttenuationFilter(Config := LightspeedConfig.SliderAttenuationFilterConfigs.ELL6SliderFilterConfig);													
	
	(* Rotating switch *) 
	ELL18RotatingSwitch : ELL18AttenuationFilter(Config := LightspeedConfig.RotatingSwitchConfigs.ELL18RotatingSwitchConfig);
	
	(* PolarisationFilterConfigs *) 
	FW212CPolarisationFilter : FW212CPolarisationFilter(Config := LightspeedConfig.PolarisationFilterConfigs.FW212CPolarisationFilterConfig);
	ELL14KPolarisationFilter : ELL14KPolarisationFilter(Config := LightspeedConfig.PolarisationFilterConfigs.ELL14KPolarisationFilterConfig, 
														HsPolarisationConfig := LsCalibrateConfig.HSPowermeter.PolarisationConfig,
														HtPolarisationConfig := LsCalibrateConfig.HTPowermeter.PolarisationConfig);
		
	
	(* LaserConfigs *) 
	SMD12Laser : SMD12Laser(Config := LightspeedConfig.LaserConfigs.SMD12LaserConfig);
	Sapphire488Laser : Sapphire488Laser(Config := LightspeedConfig.LaserConfigs.Sapphire488LaserConfig);
	
	(* FiberSwitchConfigs *) 
	EOLFiberSwitch : EOLFiberSwitch(Config := LightspeedConfig.FiberSwitchConfigs.EOLFiberSwitchConfig);
	
	(* OpticalPowermeterConfigs *) 
	PM101OpticalPowermeterHs : PM101OpticalPowermeterHs(Config := LightspeedConfig.OpticalPowermeterConfigs.PM101OpticalPowermeterConfigHs, 
														PolarisationConfig := LsCalibrateConfig.HSPowermeter.PolarisationConfig,
														RFactorSettings := LsCalibrateConfig.HSPowermeter.RFactorSettings,
														ResponsivityConfig := LsCalibrateConfig.HSPowermeter.ResponsivityConfig);
	PM101OpticalPowermeterHt : PM101OpticalPowermeterHt(Config := LightspeedConfig.OpticalPowermeterConfigs.PM101OpticalPowermeterConfigHt, 
														PolarisationConfig := LsCalibrateConfig.HTPowermeter.PolarisationConfig, 
														RFactorSettings := LsCalibrateConfig.HTPowermeter.RFactorSettings,
														ResponsivityConfig := LsCalibrateConfig.HTPowermeter.ResponsivityConfig);
	
	(* DistanceSensorConfigs *) 
	MicroEpsilonDistanceSensor : MicroEpsilonDistanceSensor(Config := LightspeedConfig.DistanceSensorConfigs.MicroEpsilonDistanceSensorConfig);
	
	(* MppcConfigs *) 
	C13336MppcWide   : C13336MppcWide(Config := LightspeedConfig.MppcConfigs.C13336MppcConfigWide);
	C13336MppcNarrow : C13366MppcNarrow(Config := LightspeedConfig.MppcConfigs.C13336MppcConfigNarrow);
	
	(* ShutterConfigs *)
	Ed12dssShutter : Ed12dssShutter(Config := LightspeedConfig.ShutterConfigs.Ed12dssShutterConfig);
	Sh10pilShutter : Sh10pilShutter(Config := LightspeedConfig.ShutterConfigs.Sh10pilShutterConfig);
	
	(* FastAttenuationConfigs *)
	Std900FastAttenuation : Std900FastAttenuation(Config := LightspeedConfig.FastAttenuationConfigs.Std900FastAttenuationConfig);	
	
END_VAR
VAR_OUTPUT
END_VAR
VAR
	{attribute 'OPC.UA.DA' := '0'}
	_startCycling : BOOL;
	{attribute 'OPC.UA.DA' := '0'}
	_count : UINT;
	{attribute 'OPC.UA.DA' := '0'}
	_ActivateCyclingMotorOpen : BOOL ; (* 0 = close door ; 1 = open door  *) 
	{attribute 'OPC.UA.DA' := '0'}
	_CyclingCounterReachSetpoint : BOOL ;
	{attribute 'OPC.UA.DA' := '0'}
	OpenCloseCyclingCount : WORD := 10; (* default value *) 
	{attribute 'OPC.UA.DA' := '0'}
	OpenCloseCurrentCounterValue : WORD:= 0; (* init value *)  ;
	{attribute 'OPC.UA.DA' := '0'}
	WaitingTimer : TON;
	{attribute 'OPC.UA.DA' := '0'}
	DoorSlitOpenCloseCounter: CTU;
	{attribute 'OPC.UA.DA' := '0'}
	OpenCloseCyclingCounterReset : BOOL := FALSE; 
	{attribute 'OPC.UA.DA' := '0'}
	_stopCycling: BOOL:= FALSE;	
	
	{attribute 'OPC.UA.DA' := '0'}
	_mppcActivate : BOOL := TRUE; //TODO à remettre 
	
END_VAR
]]></Declaration>
    <Implementation>
      <ST><![CDATA[
SUPER^();

IF LightspeedConfig.ChamberConfigs.LightspeedChamberConfig.IsEnabled THEN
	Chamber(MppcActivate => _mppcActivate);	
	
	Door_Slit_Ctrl();
	OpenCloseCycling(_startCycling, _count);
	StopCycling(_stopCycling);	
END_IF

(* Attenuation *)
IF LightspeedConfig.AttenuationFilterConfigs.FW212CAttenuationFilterConfig.IsEnabled THEN		
	FW212CAttenuationFilter();
	IF FW212CAttenuationFilter.InitConfigSerialCom THEN
		;
	END_IF
ELSIF LightspeedConfig.AttenuationFilterConfigs.ELL14KAttenuationFilterConfig.IsEnabled THEN		
	ELL14KAttenuationFilter();
	IF ELL14KAttenuationFilter.InitConfigSerialCom THEN
		;
	END_IF
ELSIF LightspeedConfig.AttenuationFilterConfigs.ELL9AttenuationFilterConfig.IsEnabled THEN		
	ELL9AttenuationFilter();
	IF ELL9AttenuationFilter.InitConfigSerialCom THEN
		;
	END_IF		
END_IF

(* Slider Attenuation *)
IF LightspeedConfig.Version = '2' THEN
	IF LightspeedConfig.SliderAttenuationFilterConfigs.ELL6SliderFilterConfig.IsEnabled THEN		
		ELL6SliderAttenuationFilter();
		IF ELL6SliderAttenuationFilter.InitConfigSerialCom THEN
			;
		END_IF
	END_IF
END_IF

(* Rotating switch *)
IF LightspeedConfig.Version = '2' THEN
	IF LightspeedConfig.RotatingSwitchConfigs.ELL18RotatingSwitchConfig.IsEnabled THEN		
		ELL18RotatingSwitch();
		IF ELL18RotatingSwitch.InitConfigSerialCom THEN
			;
		END_IF
	END_IF
END_IF

IF LightspeedConfig.PolarisationFilterConfigs.FW212CPolarisationFilterConfig.IsEnabled THEN
	FW212CPolarisationFilter();	
	IF FW212CPolarisationFilter.InitConfigSerialCom THEN
		;
	END_IF
ELSIF LightspeedConfig.PolarisationFilterConfigs.ELL14KPolarisationFilterConfig.IsEnabled THEN		
	ELL14KPolarisationFilter();
	IF ELL14KPolarisationFilter.InitConfigSerialCom THEN
		;
	END_IF			
END_IF

IF LightspeedConfig.LaserConfigs.SMD12LaserConfig.IsEnabled THEN
	SMD12Laser();
	IF SMD12Laser.InitConfigSerialCom THEN
		;
	END_IF
ELSIF LightspeedConfig.LaserConfigs.Sapphire488LaserConfig.IsEnabled THEN
	Sapphire488Laser();
	IF Sapphire488Laser.InitConfigSerialCom THEN
		;
	END_IF
ELSIF LightspeedConfig.LaserConfigs.Sapphire488LaserConfig.IsEnabled THEN
	Sapphire488Laser();
	IF Sapphire488Laser.InitConfigSerialCom THEN
		;
	END_IF	
END_IF

IF LightspeedConfig.FiberSwitchConfigs.EOLFiberSwitchConfig.IsEnabled THEN
	EOLFiberSwitch();
	IF EOLFiberSwitch.InitConfigSerialCom THEN
		;
	END_IF
END_IF

IF LightspeedConfig.OpticalPowermeterConfigs.PM101OpticalPowermeterConfigHs.IsEnabled OR 
   LightspeedConfig.OpticalPowermeterConfigs.PM101OpticalPowermeterConfigHs.IsSimulated THEN
	PM101OpticalPowermeterHs(PolarResult := ELL14KPolarisationFilter.PolarResult);
	IF PM101OpticalPowermeterHs.InitConfigSerialCom THEN
		;
	END_IF	
END_IF

IF LightspeedConfig.OpticalPowermeterConfigs.PM101OpticalPowermeterConfigHt.IsEnabled THEN
	PM101OpticalPowermeterHt(PolarResult := ELL14KPolarisationFilter.PolarResult);
	IF PM101OpticalPowermeterHt.InitConfigSerialCom THEN
		;
	END_IF	
END_IF

IF LightspeedConfig.DistanceSensorConfigs.MicroEpsilonDistanceSensorConfig.IsEnabled THEN
	MicroEpsilonDistanceSensor();	
END_IF

IF LightspeedConfig.MppcConfigs.C13336MppcConfigWide.IsEnabled THEN
	C13336MppcWide(MppcActivate := _mppcActivate);
	IF C13336MppcWide.InitConfigSerialCom THEN
		;
	END_IF
END_IF

IF LightspeedConfig.MppcConfigs.C13336MppcConfigNarrow.IsEnabled THEN
	C13336MppcNarrow(MppcActivate := _mppcActivate);
	IF C13336MppcNarrow.InitConfigSerialCom THEN
		;
	END_IF
END_IF

IF LightspeedConfig.ShutterConfigs.Ed12dssShutterConfig.IsEnabled THEN
	Ed12dssShutter();	
ELSIF LightspeedConfig.ShutterConfigs.SH10PILShutterConfig.IsEnabled THEN
	Sh10pilShutter();
END_IF

IF LightspeedConfig.FastAttenuationConfigs.Std900FastAttenuationConfig.IsEnabled THEN
	Std900FastAttenuation();	
END_IF
]]></ST>
    </Implementation>
