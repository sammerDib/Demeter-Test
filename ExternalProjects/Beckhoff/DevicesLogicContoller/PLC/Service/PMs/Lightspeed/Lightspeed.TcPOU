    <Method Name="FB_init" Id="{09564059-546a-497c-9b54-4b93903ae9de}">
      <Declaration><![CDATA[METHOD FB_init : BOOL
VAR_INPUT
	bInitRetains : BOOL; // if TRUE, the retain variables are initialized (warm start / cold start)
	bInCopyCode : BOOL;  // if TRUE, the instance afterwards gets moved into the copy code (online change)
	Config : LightspeedConfig;
	CalibrateConfig : LsCalibrateConfig;
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[LightspeedConfig := Config;
LsCalibrateConfig := CalibrateConfig;
LsBkgrdLsSerialCom(Config := LightspeedConfig);

IF Config.IsEnabled THEN
	_logger.Information('Lightspeed devices initialization starting...');
END_IF	

(* Chamber *)
IF Config.ChamberConfigs.LightspeedChamberConfig.IsEnabled THEN
	Chamber.FB_init(FALSE, FALSE, Config := Config.ChamberConfigs.LightspeedChamberConfig);	
END_IF

(* AttenuationFilterConfigs *)
IF Config.AttenuationFilterConfigs.FW212CAttenuationFilterConfig.IsEnabled THEN
	FW212CAttenuationFilter.FB_init(FALSE, FALSE, Config := Config.AttenuationFilterConfigs.FW212CAttenuationFilterConfig);	
ELSIF Config.AttenuationFilterConfigs.ELL14KAttenuationFilterConfig.IsEnabled THEN
	ELL14KAttenuationFilter.FB_init(FALSE, FALSE, Config := Config.AttenuationFilterConfigs.ELL14KAttenuationFilterConfig);
END_IF

(* Slider Attenuation *)
IF Config.Version = '2' THEN
	IF Config.SliderAttenuationFilterConfigs.ELL6SliderFilterConfig.IsEnabled THEN
		ELL6SliderAttenuationFilter.FB_init(FALSE, FALSE, Config := Config.SliderAttenuationFilterConfigs.ELL6SliderFilterConfig);
	END_IF
END_IF

(* Rotating switch *)
IF Config.Version = '2' THEN
	IF Config.RotatingSwitchConfigs.ELL18RotatingSwitchConfig.IsEnabled THEN
		ELL18RotatingSwitch.FB_init(FALSE, FALSE, Config := Config.RotatingSwitchConfigs.ELL18RotatingSwitchConfig);
	END_IF
END_IF

(* PolarisationFilterConfigs *)
IF Config.PolarisationFilterConfigs.FW212CPolarisationFilterConfig.IsEnabled THEN
	FW212CPolarisationFilter.FB_init(FALSE, FALSE, Config := Config.PolarisationFilterConfigs.FW212CPolarisationFilterConfig);	
ELSIF Config.PolarisationFilterConfigs.ELL14KPolarisationFilterConfig.IsEnabled THEN
	ELL14KPolarisationFilter.FB_init(FALSE, FALSE, Config := Config.PolarisationFilterConfigs.ELL14KPolarisationFilterConfig, 
												   HsPolarisationConfig := CalibrateConfig.HSPowermeter.PolarisationConfig,
												   HtPolarisationConfig := CalibrateConfig.HTPowermeter.PolarisationConfig);
END_IF

(* LaserConfigs *) 
IF Config.LaserConfigs.SMD12LaserConfig.IsEnabled THEN
	SMD12Laser.FB_init(FALSE, FALSE, Config := Config.LaserConfigs.SMD12LaserConfig);	
ELSIF Config.LaserConfigs.Sapphire488LaserConfig.IsEnabled THEN
	Sapphire488Laser.FB_init(FALSE, FALSE, Config := Config.LaserConfigs.Sapphire488LaserConfig);	
END_IF

(* FiberSwitchConfigs *) 
IF Config.FiberSwitchConfigs.EOLFiberSwitchConfig.IsEnabled THEN
	EOLFiberSwitch.FB_init(FALSE, FALSE, Config := Config.FiberSwitchConfigs.EOLFiberSwitchConfig);	
END_IF

(* OpticalPowermeterConfigs *) 
IF Config.OpticalPowermeterConfigs.PM101OpticalPowermeterConfigHs.IsEnabled THEN
	PM101OpticalPowermeterHs.FB_init(FALSE, FALSE, Config := Config.OpticalPowermeterConfigs.PM101OpticalPowermeterConfigHs, 
												   PolarisationConfig := CalibrateConfig.HSPowermeter.PolarisationConfig, 
												   RFactorSettings := CalibrateConfig.HSPowermeter.RFactorSettings,
												   ResponsivityConfig := CalibrateConfig.HSPowermeter.ResponsivityConfig);		
END_IF
IF Config.OpticalPowermeterConfigs.PM101OpticalPowermeterConfigHt.IsEnabled THEN
	PM101OpticalPowermeterHt.FB_init(FALSE, FALSE, Config := Config.OpticalPowermeterConfigs.PM101OpticalPowermeterConfigHt, 
												   PolarisationConfig := CalibrateConfig.HTPowermeter.PolarisationConfig,
												   RFactorSettings := CalibrateConfig.HTPowermeter.RFactorSettings,
												   ResponsivityConfig := CalibrateConfig.HTPowermeter.ResponsivityConfig);
END_IF

(* DistanceSensorConfigs *) 
IF Config.DistanceSensorConfigs.MicroEpsilonDistanceSensorConfig.IsEnabled THEN
	MicroEpsilonDistanceSensor.FB_init(FALSE, FALSE, Config := Config.DistanceSensorConfigs.MicroEpsilonDistanceSensorConfig);	
END_IF

(* MppcConfigs *) 
IF Config.MppcConfigs.C13336MppcConfigWide.IsEnabled THEN
	C13336MppcWide.FB_init(FALSE, FALSE, Config := Config.MppcConfigs.C13336MppcConfigWide);	
END_IF
IF Config.MppcConfigs.C13336MppcConfigNarrow.IsEnabled THEN
	C13336MppcNarrow.FB_init(FALSE, FALSE, Config := Config.MppcConfigs.C13336MppcConfigNarrow);	
END_IF

(* ShutterConfigs *) 
IF Config.ShutterConfigs.Ed12dssShutterConfig.IsEnabled THEN
	Ed12dssShutter.FB_init(FALSE, FALSE, Config := Config.ShutterConfigs.Ed12dssShutterConfig);	
ELSIF Config.ShutterConfigs.SH10PILShutterConfig.IsEnabled THEN
	Sh10pilShutter.FB_init(FALSE, FALSE, Config := Config.ShutterConfigs.SH10PILShutterConfig);	
END_IF

IF Config.IsEnabled THEN
	_logger.Information('Lightspeed devices initialization ended...');
END_IF	]]></ST>
      </Implementation>
    </Method>
    <Method Name="OpenCloseCycling" Id="{8a380f41-0443-4c66-a6ce-51982bdb92a2}" FolderPath="DoorSlit\">
      <Declaration><![CDATA[METHOD OpenCloseCycling : BOOL
VAR_INPUT
	Execute : BOOL;
	Count : UINT;
END_VAR
VAR 
END_VAR

]]></Declaration>
      <Implementation>
        <ST><![CDATA[IF Execute THEN
	(* timer pour l'enchainement des open/close *)
	(* WaitingTimer.in := TRUE;
	WaitingTimer.PT := T#4S;*)
	WaitingTimer (IN :=TRUE , PT := T#5S,Q=>, ET=> );
	
	(* WaitingTimer.Q is TRUE, PT seconds after IN had a rising edge *)
	IF WaitingTimer.Q THEN
		(* RAZ du timer *)
		WaitingTimer (IN := FALSE);
		(* coupe le motor ON par securité*)
		Chamber.IoLightspeed.OutputDoorslitMotorOn := FALSE;
		(* MAJ du compteur*)
		OpenCloseCyclingCount := Count;
		(* porte ouverte => on demande la fermeture*)
		IF (_CyclingCounterReachSetpoint) THEN
			(* arret du cycling et re init des variables*)
			Chamber.IoLightspeed.OutputDoorslitMotorOn := FALSE;
			_startCycling := FALSE;
			_CyclingCounterReachSetpoint := FALSE;
			_ActivateCyclingMotorOpen := FALSE;
			OpenCloseCycling := FALSE;
			OpenCloseCurrentCounterValue := 0;
			(* to do lever un timeout vers acquisition si not(_CyclingCounterReachSetpoint) => pb d'open/close  *)
		ELSIF Chamber.IoLightspeed.InputDoorslitOpened THEN
			_ActivateCyclingMotorOpen := FALSE;	
		(* porte fermée => on demande l'ouverture *)			
		ELSIF (Chamber.IoLightspeed.InputDoorslitClosed) THEN
			_ActivateCyclingMotorOpen := TRUE;	
		END_IF
	END_IF
END_IF


]]></ST>
      </Implementation>
    </Method>
    <Method Name="StopCycling" Id="{74208cd1-90d0-4c46-8202-5e5729222b64}" FolderPath="DoorSlit\">
      <Declaration><![CDATA[METHOD StopCycling : BOOL
VAR_INPUT
	Execute : BOOL;
END_VAR
VAR 
END_VAR

]]></Declaration>
      <Implementation>
        <ST><![CDATA[IF Execute THEN
	(* stop du cycling*)
	(* on attend le porte fermée pour arreter le cycling *)
	IF ( NOT (Chamber.IoLightspeed.InputDoorslitOpened) AND Chamber.IoLightspeed.InputDoorslitClosed)  THEN
		(* arret du cycling et re init des variables*)
		(* arret du cycling *)
		_startCycling := FALSE;
		(* coupe le moteur par securité *)
		Chamber.IoLightspeed.OutputDoorslitMotorOn := FALSE;
		(* fin du stop cycling*)
		_stopCycling:= FALSE;
		(* re init du compteur de cycle *)
		_CyclingCounterReachSetpoint := FALSE;
		OpenCloseCurrentCounterValue := 0;
		OpenCloseCyclingCount := 0;
	END_IF
END_IF


]]></ST>
      </Implementation>
    </Method>
